<?xml version="1.0"?>
<package format="2">
  <name>pandora_vision_landoltc</name>
  <version>0.1.0</version>
  <description>Containts landoltC detection node.</description>

  <maintainer email="elendorial@gmail.com">Victor Daropoulos</maintainer>

  <license>BSD</license>

  <url type="bugtracker">https://github.com/pandora-auth-ros-pkg/pandora_vision/issues</url>
