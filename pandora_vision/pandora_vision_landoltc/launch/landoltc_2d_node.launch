<launch>
  
  <node name="landoltc_2d_node" type="pandora_vision_landoltc_2d_node" pkg="pandora_vision_landoltc"
    respawn="False" ns="/pandora_vision"/>
    
  <rosparam file="$(find pandora_vision_landoltc)/config/pandora_vision_landoltc/landoltc_2d_topics.yaml" 
    command="load" ns="/pandora_vision/pandora_vision_landoltc_2d"/>
  <rosparam file="$(find pandora_vision_landoltc)/config/pandora_vision_landoltc/landoltc_params.yaml" 
    command="load" ns="/pandora_vision/pandora_vision_landoltc_2d"/>
        
  <param name="patternPath" type="str" value="$(find pandora_vision_landoltc)/bold.jpg"/>   
    
</launch>
